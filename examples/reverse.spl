// Make a reversed copy of any list
reverse ( list ) :: [t] -> [t] {
    var accu = [];
    while ( ! isEmpty ( list ) ) {
        accu = list.hd : accu ;
        list = list.tl;
    }
    return accu ;
}

main() {
    var list = 1 : 2 : [];
    var reverse_list = reverse(list);

    print(reverse_list.hd);
}
