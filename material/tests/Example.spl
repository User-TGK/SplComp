[Int] dcLengthOfMonth = 0 : 31 : 28 : 31 : 30 : 31 : 30 : 31 : 31 : 30 : 31 : 30 : 31 : [];

getIndex (idx, list) :: Int [a] -> a
{
	while (idx > 0) {
		list = list.tl;
	}
	return list.hd;
}


